gitlab:
  secret: <APP_SECRET>
  clientId: <APP_CLIENT_ID>

endpoints:
  hostname: https://<HOSTNAME>
  home: /
  login: /login
  logout: /logout
  signup: /signup
  oauthCallback: /finish

server:
  listenAddress: ":18080"
  cookies:
    authenticationKey: <COOKIE_AUTH_KEY>
    encryptionKey: <COOKIE_ENCRYPTION_KEY>
